                    else if (mDieuHanhTaxi.LoaiXe.Contains("7")) strXeCho += " xe, 7 chỗ";
                    else if (mDieuHanhTaxi.LoaiXe.Contains("0")) strXeCho += " xe, 4 hoặc 7 chỗ";
